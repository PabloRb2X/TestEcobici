# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Ecobici' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Ecobici
    pod 'GoogleMaps'
    pod 'GooglePlaces'

  target 'EcobiciTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces'
  end

  target 'EcobiciUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'GoogleMaps'
    pod 'GooglePlaces'
  end

end
